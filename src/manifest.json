{
  "manifest_version": 3,
  "name": "Cyrillic URL Detector",
  "version": "1.0",
  "description": "Detects Cyrillic letters in URLs and shows a warning",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
